<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupInscriptionForum" table="afup_inscription_forum">
    <indexes>
      <index name="id_forum" columns="id_forum"/>
      <index name="reference" columns="reference"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="integer" column="date" nullable="false"/>
    <field name="reference" type="string" column="reference" length="255" nullable="false"/>
    <field name="coupon" type="string" column="coupon" length="255" nullable="false"/>
    <field name="typeInscription" type="boolean" column="type_inscription" nullable="false"/>
    <field name="montant" type="float" column="montant" precision="10" scale="0" nullable="false"/>
    <field name="informationsReglement" type="string" column="informations_reglement" length="255" nullable="true"/>
    <field name="civilite" type="string" column="civilite" length="4" nullable="false"/>
    <field name="nom" type="string" column="nom" length="40" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="40" nullable="false"/>
    <field name="email" type="string" column="email" length="100" nullable="false"/>
    <field name="telephone" type="string" column="telephone" length="40" nullable="true"/>
    <field name="citerSociete" type="boolean" column="citer_societe" nullable="false"/>
    <field name="newsletterAfup" type="boolean" column="newsletter_afup" nullable="false"/>
    <field name="newsletterNexen" type="boolean" column="newsletter_nexen" nullable="false"/>
    <field name="commentaires" type="text" column="commentaires" nullable="true"/>
    <field name="etat" type="boolean" column="etat" nullable="false"/>
    <field name="facturation" type="boolean" column="facturation" nullable="false"/>
    <field name="idForum" type="smallint" column="id_forum" nullable="false"/>
    <field name="mobiliteReduite" type="boolean" column="mobilite_reduite" nullable="false"/>
    <field name="mailPartenaire" type="boolean" column="mail_partenaire" nullable="false"/>
  </entity>
</doctrine-mapping>
