<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupPlaneteFlux" table="afup_planete_flux">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="true"/>
    <field name="url" type="string" column="url" length="255" nullable="true"/>
    <field name="feed" type="string" column="feed" length="255" nullable="true"/>
    <field name="etat" type="boolean" column="etat" nullable="true"/>
    <field name="idPersonnePhysique" type="smallint" column="id_personne_physique" nullable="true"/>
  </entity>
</doctrine-mapping>
