<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupSiteRubrique" table="afup_site_rubrique">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idParent" type="integer" column="id_parent" nullable="true"/>
    <field name="nom" type="text" column="nom" nullable="true"/>
    <field name="raccourci" type="string" column="raccourci" length="255" nullable="true"/>
    <field name="contenu" type="text" column="contenu" nullable="true"/>
    <field name="descriptif" type="text" column="descriptif" nullable="true"/>
    <field name="position" type="integer" column="position" nullable="true"/>
    <field name="date" type="integer" column="date" nullable="true"/>
    <field name="etat" type="boolean" column="etat" nullable="true"/>
    <field name="idPersonnePhysique" type="smallint" column="id_personne_physique" nullable="true"/>
    <field name="icone" type="string" column="icone" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
