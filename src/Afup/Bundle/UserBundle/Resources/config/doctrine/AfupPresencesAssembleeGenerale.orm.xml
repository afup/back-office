<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupPresencesAssembleeGenerale" table="afup_presences_assemblee_generale">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idPersonnePhysique" type="smallint" column="id_personne_physique" nullable="true"/>
    <field name="date" type="integer" column="date" nullable="false"/>
    <field name="presence" type="boolean" column="presence" nullable="false"/>
    <field name="idPersonneAvecPouvoir" type="smallint" column="id_personne_avec_pouvoir" nullable="false"/>
    <field name="dateConsultation" type="integer" column="date_consultation" nullable="true"/>
    <field name="dateModification" type="integer" column="date_modification" nullable="true"/>
  </entity>
</doctrine-mapping>
