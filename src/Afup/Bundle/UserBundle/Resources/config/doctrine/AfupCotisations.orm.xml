<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupCotisations" table="afup_cotisations">
    <indexes>
      <index name="id_personne" columns="id_personne"/>
    </indexes>
    <id name="id" type="smallint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateDebut" type="integer" column="date_debut" nullable="false"/>
    <field name="typePersonne" type="boolean" column="type_personne" nullable="false"/>
    <field name="idPersonne" type="smallint" column="id_personne" nullable="false"/>
    <field name="montant" type="float" column="montant" precision="5" scale="2" nullable="false"/>
    <field name="typeReglement" type="boolean" column="type_reglement" nullable="false"/>
    <field name="informationsReglement" type="string" column="informations_reglement" length="255" nullable="true"/>
    <field name="dateFin" type="integer" column="date_fin" nullable="false"/>
    <field name="numeroFacture" type="string" column="numero_facture" length="15" nullable="false"/>
    <field name="commentaires" type="text" column="commentaires" nullable="true"/>
    <field name="nombreRelances" type="boolean" column="nombre_relances" nullable="true"/>
    <field name="dateDerniereRelance" type="integer" column="date_derniere_relance" nullable="true"/>
  </entity>
</doctrine-mapping>
