<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupConferenciers" table="afup_conferenciers">
    <indexes>
      <index name="id_forum" columns="id_forum"/>
    </indexes>
    <id name="conferencierId" type="integer" column="conferencier_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idForum" type="smallint" column="id_forum" nullable="false"/>
    <field name="civilite" type="string" column="civilite" length="5" nullable="false"/>
    <field name="nom" type="string" column="nom" length="70" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="50" nullable="false"/>
    <field name="email" type="string" column="email" length="65" nullable="false"/>
    <field name="societe" type="string" column="societe" length="120" nullable="true"/>
    <field name="biographie" type="text" column="biographie" nullable="false"/>
    <field name="twitter" type="string" column="twitter" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
