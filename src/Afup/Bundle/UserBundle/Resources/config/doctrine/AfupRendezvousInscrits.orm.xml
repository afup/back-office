<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupRendezvousInscrits" table="afup_rendezvous_inscrits">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idRendezvous" type="integer" column="id_rendezvous" nullable="true"/>
    <field name="nom" type="string" column="nom" length="255" nullable="true"/>
    <field name="prenom" type="string" column="prenom" length="100" nullable="false"/>
    <field name="entreprise" type="string" column="entreprise" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="telephone" type="string" column="telephone" length="255" nullable="true"/>
    <field name="presence" type="boolean" column="presence" nullable="true"/>
    <field name="confirme" type="boolean" column="confirme" nullable="true"/>
    <field name="creation" type="integer" column="creation" nullable="true"/>
  </entity>
</doctrine-mapping>
