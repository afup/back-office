<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\Member" table="afup_personnes_physiques">
    <indexes>
      <index name="pays" columns="id_pays"/>
      <index name="personne_morale" columns="id_personne_morale"/>
    </indexes>
    <id name="id" type="smallint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idPersonneMorale" type="smallint" column="id_personne_morale" nullable="false"/>
    <field name="login" type="string" column="login" length="30" nullable="false"/>
    <field name="motDePasse" type="string" column="mot_de_passe" length="32" nullable="false"/>
    <field name="niveau" type="boolean" column="niveau" nullable="false"/>
    <field name="niveauModules" type="string" column="niveau_modules" length="10" nullable="false"/>
    <field name="civilite" type="string" column="civilite" length="4" nullable="false"/>
    <field name="nom" type="string" column="nom" length="40" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="40" nullable="false"/>
    <field name="email" type="string" column="email" length="100" nullable="false"/>
    <field name="adresse" type="text" column="adresse" nullable="false"/>
    <field name="codePostal" type="string" column="code_postal" length="10" nullable="false"/>
    <field name="ville" type="string" column="ville" length="50" nullable="false"/>
    <field name="idPays" type="string" column="id_pays" length="2" nullable="false"/>
    <field name="telephoneFixe" type="string" column="telephone_fixe" length="20" nullable="true"/>
    <field name="telephonePortable" type="string" column="telephone_portable" length="20" nullable="true"/>
    <field name="etat" type="boolean" column="etat" nullable="false"/>
    <field name="dateRelance" type="integer" column="date_relance" nullable="true"/>
    <field name="compteSvn" type="string" column="compte_svn" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
