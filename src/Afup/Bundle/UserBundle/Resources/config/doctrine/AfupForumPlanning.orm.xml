<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupForumPlanning" table="afup_forum_planning">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idSession" type="integer" column="id_session" nullable="true"/>
    <field name="debut" type="integer" column="debut" nullable="true"/>
    <field name="fin" type="integer" column="fin" nullable="true"/>
    <field name="idSalle" type="smallint" column="id_salle" nullable="true"/>
    <field name="idForum" type="integer" column="id_forum" nullable="true"/>
    <field name="keynote" type="boolean" column="keynote" nullable="false"/>
  </entity>
</doctrine-mapping>
