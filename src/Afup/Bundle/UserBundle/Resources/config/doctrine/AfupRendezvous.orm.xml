<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupRendezvous" table="afup_rendezvous">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="255" nullable="true"/>
    <field name="accroche" type="text" column="accroche" nullable="true"/>
    <field name="theme" type="text" column="theme" nullable="true"/>
    <field name="debut" type="integer" column="debut" nullable="true"/>
    <field name="fin" type="integer" column="fin" nullable="true"/>
    <field name="lieu" type="string" column="lieu" length="255" nullable="true"/>
    <field name="url" type="string" column="url" length="255" nullable="false"/>
    <field name="plan" type="string" column="plan" length="255" nullable="false"/>
    <field name="adresse" type="text" column="adresse" nullable="false"/>
    <field name="capacite" type="integer" column="capacite" nullable="true"/>
    <field name="idAntenne" type="integer" column="id_antenne" nullable="false"/>
    <field name="inscription" type="boolean" column="inscription" nullable="false"/>
    <field name="urlExterne" type="string" column="url_externe" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
