<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupSessions" table="afup_sessions">
    <id name="sessionId" type="integer" column="session_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idForum" type="smallint" column="id_forum" nullable="false"/>
    <field name="dateSoumission" type="date" column="date_soumission" nullable="false"/>
    <field name="titre" type="string" column="titre" length="255" nullable="false"/>
    <field name="abstract" type="text" column="abstract" nullable="false"/>
    <field name="journee" type="boolean" column="journee" nullable="false"/>
    <field name="genre" type="boolean" column="genre" nullable="false"/>
    <field name="plannifie" type="boolean" column="plannifie" nullable="true"/>
    <field name="joindin" type="integer" column="joindin" nullable="true"/>
  </entity>
</doctrine-mapping>
