<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupComptaFactureDetails" table="afup_compta_facture_details">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idafupComptaFacture" type="integer" column="idafup_compta_facture" nullable="false"/>
    <field name="ref" type="string" column="ref" length="20" nullable="false"/>
    <field name="designation" type="string" column="designation" length="100" nullable="false"/>
    <field name="quantite" type="float" column="quantite" precision="11" scale="2" nullable="false"/>
    <field name="pu" type="float" column="pu" precision="11" scale="2" nullable="false"/>
  </entity>
</doctrine-mapping>
