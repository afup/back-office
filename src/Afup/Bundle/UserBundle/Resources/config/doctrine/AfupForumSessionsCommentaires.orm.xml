<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupForumSessionsCommentaires" table="afup_forum_sessions_commentaires">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idSession" type="integer" column="id_session" nullable="true"/>
    <field name="idPersonnePhysique" type="integer" column="id_personne_physique" nullable="true"/>
    <field name="commentaire" type="text" column="commentaire" nullable="true"/>
    <field name="date" type="integer" column="date" nullable="true"/>
    <field name="public" type="boolean" column="public" nullable="true"/>
  </entity>
</doctrine-mapping>
