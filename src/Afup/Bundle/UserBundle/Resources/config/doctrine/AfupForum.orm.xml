<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Afup\Bundle\UserBundle\Entity\AfupForum" table="afup_forum">
    <id name="id" type="smallint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="50" nullable="false"/>
    <field name="path" type="string" column="path" length="100" nullable="true"/>
    <field name="nbPlaces" type="integer" column="nb_places" nullable="false"/>
    <field name="dateDebut" type="date" column="date_debut" nullable="false"/>
    <field name="dateFin" type="date" column="date_fin" nullable="false"/>
    <field name="annee" type="integer" column="annee" nullable="true"/>
    <field name="dateFinAppelProjet" type="integer" column="date_fin_appel_projet" nullable="true"/>
    <field name="dateFinAppelConferencier" type="integer" column="date_fin_appel_conferencier" nullable="true"/>
    <field name="dateFinPrevente" type="integer" column="date_fin_prevente" nullable="true"/>
    <field name="dateFinVente" type="integer" column="date_fin_vente" nullable="true"/>
  </entity>
</doctrine-mapping>
